         "library": false
      }
   },
   "scripts": {
      "web-server": "http-server -p 8366 &",
      "pretest": "jshint gulpfile.js websites",
      "test": "gulp clean && gulp web && gulp lint"
   },
   "dependencies": {
      "dna.js": "~1.6",
      "hamburger-menu": "~0.3",
      "jquery": "~3.5",
      "web-ignition": "~1.2"
   },
   "devDependencies": {
      "del": "~5.1",
      "gulp": "~4.0",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.1",
      "gulp-htmlhint": "~3.0",
      "gulp-jshint": "~2.1",
      "gulp-rename": "~2.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~2.0",
      "gulp-zip": "~5.0",
      "http-server": "~0.12",
      "jshint": "~2.11",
      "merge-stream": "~2.0"
   }
}
