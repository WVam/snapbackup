{
   "name": "snapbackup",
   "version": "6.5.0",
   "description": "The one-click backup utility",
   "license": "GPL-3.0",
   "type": "module",
   "main": "releases/snapbackup.jar",
   "repository": "github:snap-backup/snapbackup",
   "homepage": "https://github.com/snap-backup/snapbackup",
   "bugs": "https://github.com/snap-backup/snapbackup/issues",
   "docs": "https://snapbackup.org/faq",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "backup",
      "utility",
      "zip",
      "internationalization",
      "java"
   ],
   "extra": {
      "updated": "February 15, 2022",
      "jarSize": "232 KB"
   },
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "jquery": true,
      "node": true,
      "globals": {
         "dna": false,
         "libX": false
      }
   },
   "runScriptsConfig": {
      "build": [
         "rimraf website-target **/.DS_Store",
         "jshint . --exclude-path .gitignore",
         "replacer website/root website-target --pkg",
         "copy-file src/resources/snap-backup-user-guide.html --folder website-target",
         "copy-folder src/resources/graphics/application --ext=.png website-target/graphics/flags",
         "copy-folder website/graphics --ext=.png,.svg website-target/graphics",
         "lessc website/snap-backup.less website-target/style.css",
         "uglifyjs website/snap-backup.js --output website-target/app.js"
      ]
   },
   "scripts": {
      "pretest": "run-scripts build",
      "test": "html-validator website-target",
      "interactive": "browser-sync website-target --watch"
   },
   "cdnVersion": {
      "fontAwesome": "6.2"
   },
   "devDependencies": {
      "browser-sync": "~2.27",
      "copy-file-util": "~0.1",
      "copy-folder-util": "~0.2",
      "dna-engine": "~2.2",
      "hamburger-menu": "~0.5",
      "jquery": "~3.6",
      "jshint": "~2.13",
      "less": "~4.1",
      "replacer-util": "~0.3",
      "rimraf": "~3.0",
      "run-scripts-util": "~0.1",
      "uglify-js": "~3.17",
      "w3c-html-validator": "~1.2",
      "web-ignition": "~1.6"
   }
}
